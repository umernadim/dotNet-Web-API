@model Student;
<div class="container">
<h1 class="text-center">Add Student</h1>

    <form asp-action="AddStudent" method="post">
        <div class="form-outline mb-4">
            <input type="text" id="form1Example1" class="form-control" asp-for="StudName"/>
            <label class="form-label" for="form1Example1">Student Name</label>
        </div>

        <div class="form-outline mb-4">
            <input type="text" id="form1Example2" class="form-control" asp-for="Age"/>
            <label class="form-label" for="form1Example2"> Age</label>
        </div>
        <div class="form-outline mb-4">
            <input type="text" id="form1Example2" class="form-control" asp-for="Gender"/>
            <label class="form-label" for="form1Example2"> Gender</label>
        </div>
        <button type="submit" class="btn btn-primary btn-block">Add Student</button>

    </form>
</div>


